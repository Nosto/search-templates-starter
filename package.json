  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "keywords": [],
  "author": "Nosto",
  "scripts": {
    "dev:injected": "vite --mode injected",
    "dev": "vite --mode native",
