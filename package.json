    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "preact": "^10.27.1",
    "prettier": "^3.3.3",
    "storybook": "^9.1.3",
    "typedoc": "^0.28.12",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.41.0",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
