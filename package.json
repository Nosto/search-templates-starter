    "@preact/preset-vite": "^2.10.2",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/preact-vite": "^9.1.3",
    "@testing-library/dom": "^10.4.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.8.7",
    "copyfiles": "^2.4.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
